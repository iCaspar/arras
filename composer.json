{
  "name": "icaspar/arras",
  "description": "Arras WordPress Theme",
  "type": "Theme",
  "keywords": [
    "wordpress",
    "wp",
    "theme"
  ],
  "homepage": "https://caspar.green",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Caspar Green",
      "email": "cg@caspar.green"
    }
  ],
  "require": {
    "php": ">=5.6.0"
  },
  "require-dev": {
    "phpspec/phpspec": "^3.0",
    "10up/wp_mock": "dev-master",
    "raveren/kint": "~1.0",
    "filp/whoops": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "ICaspar\\Arras\\": "src/"
    }
  },
  "autoload-dev": {
    "files": [
      "inc/actions.php",
      "inc/filters.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "minimum-stability": "dev"
}